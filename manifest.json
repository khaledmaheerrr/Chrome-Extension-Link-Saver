{
  "manifest_version": 3,
  "name": "Link Saver",
  "version": "1.0",
  "description": "Save your favorite links easily.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": ["storage", "tabs"]
}
